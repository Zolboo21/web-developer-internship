<script type="text/javascript">
function initialize() {
	var salbars = [
					['IAG', 47.920851,106.954117, 'map_canvasc'],
				];
	var maps = [{}, {}, {}], markers = [{}, {}, {}], myZoom = 16;
	for(var i=0; i<salbars.length; i++)
	{
		var coord = new google.maps.LatLng(salbars[i][1], salbars[i][2]);
		maps[i] = new google.maps.Map(document.getElementById(salbars[i][3]), {
			center: coord,
			zoom: myZoom,
			mapTypeId: google.maps.MapTypeId.HYBRID,
			mapTypeControl: false,
			panControl: false,
			rotateControl: false,
			streetViewControl: false
		});
		markers[i] = new google.maps.Marker({
			position: coord,
			map: maps[i],
			title: salbars[i][0],
			icon: "../images/lmap.png",
		});
	}
};
google.maps.event.addDomListener(window, 'load', initialize);

$(document).ready(function() {

		$('.mail .input').focus(function(){  $(this).addClass('inputFocus');  });
		$('.mail .input').blur(function(){  $(this).removeClass('inputFocus');  });
		
		$('.mail #cname')
		.focus(function()  {
			if($(this).val() =='Name')  { $(this).val(''); }
		})
		$('.mail #cname')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('Name'); }
		})
		$('.mail #cphone')
		.focus(function()  {
			if($(this).val() =='Phone')  { $(this).val(''); }
		})
		$('.mail #cphone')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('Phone'); }
		})
		$('.mail #cemail')
		.focus(function()  {
			if($(this).val() =='example@yahoo.com')  { $(this).val(''); }
		})
		$('.mail #cemail')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('example@yahoo.com'); }
		})
		$('.mail #subject')
		.focus(function()  {
			if($(this).val() =='Subject')  { $(this).val(''); }
		})
		$('.mail #subject')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('Subject'); }
		})



		$("#sendemail").click(function(e){ 
			var valid = '',
				isr = ' please enter',
				cname = $("#cname").val(),
				cphone = $("#cphone").val(),
				cemail = $("#cemail").val(),
				subject = $("#subject").val(),
				cmsj = $("#cmsj").val(),
				ccapch = $("#ccapch").val();
			
			if (cname == 'name') {	valid += '-Your name'+isr; }
			if (!cemail.match(/^([a-z0-9._-]+@[a-z0-9._-]+\.[a-z]{2,4}$)/i)) { valid += '<br />-Full E-mail address'+isr; }
			if (cemail == 'example@yahoo.com') { valid += (valid != '' ? '<br />' : '')+'-E-mail address'+isr; }
			if (subject == 'Subject') { valid += (valid != '' ? '<br />' : '')+'-Subject'+isr; }cmsj
			if (cmsj.length<1) { valid += (valid != '' ? '<br />' : '')+'-Letter'+isr; }cmsj
			if (ccapch == '') { valid += (valid != '' ? '<br />' : '')+'-captcha'+isr;}
			
			if (valid!='') {
				$("#contactrmsj").addClass('contactrmsj').fadeIn("slow");
				$("#contactrmsj").html('<h1>'+valid+'</h1>');
			}
			else {
				var datastr ='t=ajax&cname=' + cname + '&cphone=' + cphone + '&cemail=' + cemail + '&subject=' + subject + '&cmsj=' + cmsj + '&ccapch='+ccapch;
				$("#contactrmsj").css("display", "block");
				$("#contactrmsj").html("Sending your letter.... ");
				$("#contactrmsj").fadeIn("slow");
				$.ajax({	
					type: "POST",
					url: "mail.php",
					data: datastr,
					cache: false,
					success: function(data, textStatus, jqXHR){
						if(jqXHR.status == 400) {
							$("#contactrmsj").addClass('contactrmsj').fadeIn("slow");
							$("#contactrmsj").html('<h1>'+jqXHR.responseText+'</h1>');
						}
						else {						
							$("#contactrmsj").fadeIn("slow");
							$("#contactrmsj").html(data);
							$("#ccapch").val('');
							$(".ccapclk a").click();
							//setTimeout('$("#contactrmsj").fadeOut("slow")',3000);
						};
					},
					error: function(jqXHR, textStatus, errorThrown){
						$("#contactrmsj").addClass('contactrmsj').fadeIn("slow");
						$("#contactrmsj").html('<h1>'+jqXHR.responseText+'</h1>');
					}
				});
			};
			return false;
		});
	});

</script></aside>
