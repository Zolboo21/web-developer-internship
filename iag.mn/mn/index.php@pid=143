<script type="text/javascript">
	$(document).ready(function() {
		$('#dte').on('change', function() {
			$.get("/mn/ajax.php?tp=hsel&dte=" + $(this).val(), function(data) {
				$('#hudlul').empty().append('<option value="">Газар хөдлөл ...</option>' + data);
			});
		});
		$('#aimag').on('change', function() {
			$.get("/mn/ajax.php?tp=qsel&k=duureg&id=" + $(this).val(), function(data) {
				$('#duureg').empty().append('<option value="">Дүүрэг, сум ...</option>' + data);
			});
			if ($(this).val() == 42) {
				$('#khoroo').fadeIn().attr('required', 'true');
				$('#street').fadeIn().attr('required', 'true');
			} else {
				$('#khoroo').removeAttr('required').fadeOut();
				$('#street').removeAttr('required').fadeOut();
			}
		});
		$('#duureg').on('change', function() {
			if ($(this).val() != '' && $('#aimag').val() == 42){
				$.get("/mn/ajax.php?tp=qsel&k=khoroo&id=" + $(this).val(), function(data) {
					$('#khoroo').empty().append('<option value=""> Хороо, баг... </option>' + data);
					alert($('#khoroo option').length);
					if ($('#khoroo option').length == 1) {
						$('#khoroo').empty().removeAttr('required').fadeOut();
						$('#street').empty().removeAttr('required').fadeOut();
					} else {
						$('#khoroo').fadeIn().attr('required', 'true');
						$('#street').fadeIn().attr('required', 'true');
					}
				});
			} else {
			    $('#khoroo').empty().removeAttr('required');
				$('#street').empty().removeAttr('required');
			}
		});
		$('#khoroo').on('change', function() {
			if ($(this).val() != '' && $('#aimag').val() == 42)
				$.get("/mn/ajax.php?tp=qsel&k=street&id=" + $(this).val(), function(data) {
					$('#street').empty().append('<option value=""> Гудамж, байр... </option>' + data);
					if ($('#street option').length == 1) {
						$('#street').empty().removeAttr('required').fadeOut();
					} else {
						$('#street').fadeIn().attr('required', 'true');
					}
				});
		});

		$('#rignupm').on('click', function() {
			var backMSG = 0;
			var eachAnsw = $('.signuppge .rror');
			eachAnsw.each(function() {
				if ($(this).val() == '') {
					$(this).addClass('rdMsj');
					backMSG = 1;
					$('#wrnmssge').show(150);
				}
			});

			$('.signuppge .rror').on('focus', function() {
				$(this).removeClass('rdMsj');
				$('#wrnmssge').hide(150);
			});

			if (backMSG == 0) {
				var formSt = document.getElementById('sendInfo');
				formSt.submit();
				return false;
			} else {

			}



		});


	});
</script></aside>
