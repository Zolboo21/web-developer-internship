<script type="text/javascript">
function initialize() {
	var salbars = [
					['ООГФХ', 47.920851,106.954117, 'map_canvasc'],
				];
	var maps = [{}, {}, {}], markers = [{}, {}, {}], myZoom = 16;
	for(var i=0; i<salbars.length; i++)
	{
		var coord = new google.maps.LatLng(salbars[i][1], salbars[i][2]);
		maps[i] = new google.maps.Map(document.getElementById(salbars[i][3]), {
			center: coord,
			zoom: myZoom,
			mapTypeId: google.maps.MapTypeId.HYBRID,
			mapTypeControl: false,
			panControl: false,
			rotateControl: false,
			streetViewControl: false
		});
		markers[i] = new google.maps.Marker({
			position: coord,
			map: maps[i],
			title: salbars[i][0],
			icon: "../images/lmap.png",
		});
	}
};
google.maps.event.addDomListener(window, 'load', initialize);

$(document).ready(function() {

		$('.mail .input').focus(function(){  $(this).addClass('inputFocus');  });
		$('.mail .input').blur(function(){  $(this).removeClass('inputFocus');  });
		
		$('.mail #cname')
		.focus(function()  {
			if($(this).val() =='Нэр')  { $(this).val(''); }
		})
		$('.mail #cname')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('Нэр'); }
		})
		$('.mail #cphone')
		.focus(function()  {
			if($(this).val() =='Утас')  { $(this).val(''); }
		})
		$('.mail #cphone')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('Утас'); }
		})
		$('.mail #cemail')
		.focus(function()  {
			if($(this).val() =='example@yahoo.com')  { $(this).val(''); }
		})
		$('.mail #cemail')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('example@yahoo.com'); }
		})
		$('.mail #subject')
		.focus(function()  {
			if($(this).val() =='Гарчиг')  { $(this).val(''); }
		})
		$('.mail #subject')
		.blur(function()  {
			if($(this).val() =='')  { $(this).val('Гарчиг'); }
		})



		$("#sendemail").click(function(e){ 
			var valid = '',
				isr = ' оруулна уу',
				cname = $("#cname").val(),
				cphone = $("#cphone").val(),
				cemail = $("#cemail").val(),
				subject = $("#subject").val(),
				cmsj = $("#cmsj").val(),
				ccapch = $("#ccapch").val();
			
			if (cname == 'Нэр') {	valid += '-Хэрэглэгчийн нэрээ'+isr; }
			if (!cemail.match(/^([a-z0-9._-]+@[a-z0-9._-]+\.[a-z]{2,4}$)/i)) { valid += '<br />-И-мэйл хаяг'+isr; }
			if (cemail == 'example@yahoo.com') { valid += (valid != '' ? '<br />' : '')+'-Өөрийн мэйл хаягаа'+isr; }
			if (subject == 'Гарчиг') { valid += (valid != '' ? '<br />' : '')+'-Гарчигаа'+isr; }cmsj
			if (cmsj.length<1) { valid += (valid != '' ? '<br />' : '')+'-Та захидалаа'+isr; }cmsj
			if (ccapch == '') { valid += (valid != '' ? '<br />' : '')+'-Та зураг дээрх үгийг'+isr;}
			
			if (valid!='') {
				$("#contactrmsj").addClass('contactrmsj').fadeIn("slow");
				$("#contactrmsj").html('<h1>'+valid+'</h1>');
			}
			else {
				var datastr ='t=ajax&cname=' + cname + '&cphone=' + cphone + '&cemail=' + cemail + '&subject=' + subject + '&cmsj=' + cmsj + '&ccapch='+ccapch;
				$("#contactrmsj").css("display", "block");
				$("#contactrmsj").html("Таны захидлыг илгээж байна .... ");
				$("#contactrmsj").fadeIn("slow");
				$.ajax({	
					type: "POST",
					url: "mail.php",
					data: datastr,
					cache: false,
					success: function(data, textStatus, jqXHR){
						if(jqXHR.status == 400) {
							$("#contactrmsj").addClass('contactrmsj').fadeIn("slow");
							$("#contactrmsj").html('<h1>'+jqXHR.responseText+'</h1>');
						}
						else {						
							$("#contactrmsj").fadeIn("slow");
							$("#contactrmsj").html(data);
							$("#ccapch").val('');
							$(".ccapclk a").click();
							//setTimeout('$("#contactrmsj").fadeOut("slow")',3000);
						};
					},
					error: function(jqXHR, textStatus, errorThrown){
						$("#contactrmsj").addClass('contactrmsj').fadeIn("slow");
						$("#contactrmsj").html('<h1>'+jqXHR.responseText+'</h1>');
					}
				});
			};
			return false;
		});
	});

</script></aside>
